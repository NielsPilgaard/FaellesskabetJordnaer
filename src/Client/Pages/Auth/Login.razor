@page "/auth/login"
<MudContainer MaxWidth="MaxWidth.Small">

    <MudStack>
        <MudPaper Elevation="5" Class="pa-4">
            <InitialRegistrationForm />
        </MudPaper>

        <MudPaper Elevation="5" Class="pa-4">
            <LoginForm />
        </MudPaper>

        <MudPaper Elevation="5" Class="pa-4">
            <ExternalLoginButtons />
        </MudPaper>
    </MudStack>

</MudContainer>
